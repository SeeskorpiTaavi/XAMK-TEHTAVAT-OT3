    <div id="laatikko"></div>

    
        <script>
            var kaupungit = ["Jyväskylä","Oulu","Turku","Espoo","Vaasa"];
                kaupungit.pop(4);
                kaupungit.push("Mikkeli");
                kaupungit[0] = "Tampere";

            var teksti = "";
            for(i = 0;i <kaupungit.length; i++){
                teksti += kaupungit[i] + "<br/>";
            }
        
        document.getElementById("laatikko").innerHTML = teksti + "<br/>";
     </script>


